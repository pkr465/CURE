# .env file for the project

# ── Paths ────────────────────────────────────────────────────────────────────
# Referenced by hitl: store_db_path: "${OUT_DIR}/hitl/feedback.db"
OUT_DIR=./out

# ── Database Credentials ─────────────────────────────────────────────────────
# Referenced by database: password: ${POSTGRES_PASSWORD}
POSTGRES_PASSWORD=postgres

# Referenced by database: admin_password: ${POSTGRES_ADMIN_PASSWORD}
POSTGRES_ADMIN_PASSWORD=postgres

# ── API Keys ─────────────────────────────────────────────────────────────────
# Referenced by llm: qgenie_api_key: ${QGENIE_API_KEY}
QGENIE_API_KEY= ""

# Referenced by llm: anthropic_api_key: ${ANTHROPIC_API_KEY}
# Required when llm.llm_provider is set to "anthropic" in global_config.yaml
ANTHROPIC_API_KEY= ""
